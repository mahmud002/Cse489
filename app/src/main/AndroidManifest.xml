<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tourplanner">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/e"
        android:label="@string/app_name"
        android:roundIcon="@drawable/e"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.TourPlanner">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupActivity"/>
        <activity android:name=".EventActivity"/>
        <activity android:name=".MyEventActivity"/>
        <activity android:name=".EventCreateActivity"/>
        <activity android:name=".EventShow"/>
        <activity android:name=".MyEventShow"/>
        <activity android:name=".EventMemList"/>
        <activity android:name=".AddMember"/>
        <activity android:name=".msgActivity"/>
        <activity android:name=".ProfileActivity"/>
        <activity android:name=".drive"/>
        <activity android:name=".editEvent"/>
        <activity android:name=".AlarmActivity"/>
        <receiver android:name=".ReminderBroadcast"/>


    </application>

</manifest>